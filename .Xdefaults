! XTerm*utf8:                    2
! XTerm.vt100.locale:            true
! XTerm*eightBitInput:           true
XTerm*saveLines:               32767
XTerm*scrollTtyKeypress:       true
XTerm*scrollTtyOutput:         false
! XTerm*scrollBar:               false
! XTerm.vt100.scrollbar.width:   12
! XTerm*loginShell:              true
! start with TrueType fonts
XTerm.vt100.renderFont: true
XTerm*faceName: Source Code Pro for Powerline:size=16
! XTerm*faceName: DejaVu Sans Mono for Powerline
! XTerm.vt100.faceName: Liberation Mono:size=20
XTerm.vt100.scaleHeight: 1.05
! XTerm.vt100.font: 7x13
! XTerm*jumpScroll:              true
! XTerm*multiScroll:             true

! XTerm*toolBar:                 false
! XTerm.vt100.geometry:          80x32
! XTerm*geometry:                80x80
! XTerm.vt100.metaSendsEscape:   true
! XTerm.vt100.backarrowKey:      false
! XTerm.ttyModes:                erase ^?

XTerm*background:              #fdf6e3
XTerm*foreground:              #657b83
XTerm*fadeColor:               #fdf6e3
XTerm*cursorColor:             #586e75
XTerm*pointerColorBackground:  #93a1a1
XTerm*pointerColorForeground:  #586e75

!! black dark/light
XTerm*color0:                #073642
XTerm*color8:                #002b36

!! red dark/light
