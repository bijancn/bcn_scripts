*customization: -color
Xcursor.size:   48
Xcursor.theme:  elementary
Xft.antialias:  1
Xft.dpi:        192
Xft.hinting:    1
Xft.hintstyle:  hintslight
Xft.rgba:       rgb

! Always UTF8
XTerm.vt100.utf8:              2
! Fix alt+. and others
XTerm*eightBitInput:           false
! Use LC_CTYPE locale encoding
XTerm.vt100.locale:            false
XTerm.vt100.saveLines:         32767
XTerm*scrollTtyKeypress:       true
XTerm*scrollTtyOutput:         false

*VT100*translations: #override Shift <Btn1Up>: exec-formatted("sensible-browser '%t'", PRIMARY)

XTerm*dynamicColors:           true

! double-click to select whole URLs :D
xterm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

! start with TrueType fonts
XTerm.vt100.renderFont: true
XTerm*faceName: Droid Sans Mono for Powerline:size=9
! XTerm*faceNameDoublesize: xft:Microsoft YaHei:size=9
! XTerm*faceName: DejaVu Sans Mono Book
! XTerm*faceSize: 11
! xterm*wideFont: -misc-fixed-medium-r-normal-ja-18-120-100-100-c-180-iso10646-1
! XTerm*faceName:monospace:pixelsize=20
! XTerm*faceName: Source Code Pro for Powerline:size=9
! XTerm*faceName: DejaVu Sans Mono for Powerline
! XTerm.vt100.faceName: Liberation Mono:size=20
XTerm.vt100.scaleHeight: 1.05
! XTerm.vt100.font: 7x13

! XTerm*scrollBar:               false
! XTerm.vt100.scrollbar.width:   12
! XTerm*loginShell:              true
! XTerm*jumpScroll:              true
! XTerm*multiScroll:             true

! XTerm*toolBar:                 false
! XTerm.vt100.geometry:          80x32
! XTerm*geometry:                80x80
! XTerm.vt100.metaSendsEscape:   true
! XTerm.vt100.backarrowKey:      false
! XTerm.ttyModes:                erase ^?

XTerm*background:              #fdf6e3
XTerm*foreground:              #657b83
XTerm*fadeColor:               #fdf6e3
XTerm*cursorColor:             #586e75
XTerm*pointerColorBackground:  #93a1a1
XTerm*pointerColorForeground:  #586e75

!! black dark/light
XTerm*color0:                #073642
XTerm*color8:                #002b36

!! red dark/light
XTerm*color1:                #dc322f
XTerm*color9:                #cb4b16

!! green dark/light
XTerm*color2:                #859900
XTerm*color10:               #586e75

!! yellow dark/light
XTerm*color3:                #b58900
XTerm*color11:               #657b83

!! blue dark/light
XTerm*color4:                #268bd2
XTerm*color12:               #839496

!! magenta dark/light
XTerm*color5:                #d33682
XTerm*color13:               #6c71c4

!! cyan dark/light
XTerm*color6:                #2aa198
XTerm*color14:               #93a1a1

!! white dark/light
XTerm*color7:                #eee8d5
XTerm*color15:               #fdf6e3
